# === AI Provider Selection ===
# Options: google, modelscope, dashscope
AI_PROVIDER=google

# === Google AI Configuration ===
GEMINI_API_KEY=your_gemini_api_key_here
IMAGEN_API_KEY=your_imagen_api_key_here

# Model Names (Optional - defaults provided)
GEMINI_MODEL_NAME=gemini-1.5-flash
IMAGEN_MODEL_NAME=imagen-4.0-fast-generate-001

# === ModelScope API-Inference Configuration (Optional) ===
# 2000 free API calls per day
# Get your API key from: https://modelscope.cn/my/myaccesstoken
MODELSCOPE_API_KEY=your_modelscope_token_here
MS_TEXT_MODEL=Qwen/Qwen3-235B-A22B-Instruct
MS_IMAGE_MODEL=qwen/Qwen-Image
MS_ENABLE_THINKING=false

# === DashScope/Alibaba Cloud Configuration (Optional) ===
# Get your API key from: https://dashscope.console.aliyun.com/apiKey
DASHSCOPE_API_KEY=your_dashscope_api_key_here
QIANWEN_MODEL_NAME=qwen-plus
WANXIANG_MODEL_NAME=qwen-image

# === MCP Service Configuration ===
# MCP Service URL (Optional - default provided)
XHS_MCP_BASE_URL=http://localhost:18060